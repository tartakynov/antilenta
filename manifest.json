{
  "name": "AntiLenta",
  "description": "Redirects from Lenta to Meduza",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "http://lenta.ru/*",
    "https://lenta.ru/*",
    "http://life.ru/*",
    "https://life.ru/*",
    "http://lifenews.ru/*",
    "https://lifenews.ru/*",
    "webRequest",
    "webRequestBlocking",
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"]
  }
}
